<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <%= link_to '#' do %> <div class="navButton container">Link1</div> <% end %>
    <%= link_to '#' do %> <div class="navButton container">Link2</div> <% end %>
    <%= link_to '#' do %> <div class="navButton container">Link3</div> <% end %>
    <%= link_to '#' do %> <div class="navButton container">Link4</div> <% end %>
    <%= link_to '#' do %> <div class="navButton container">Link5</div> <% end %>
  </nav>
  <div id="container">
    <%= yield %>
  </div>
</body>
</html>
